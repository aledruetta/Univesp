    "def somapositivos(A):\n",
    "    n = len(A)\n",
    "    if n == 0:\n",
    "        return 0\n",
    "    s = somapositivos(A[:-1])\n",
    "    n -= 1\n",
    "    if A[n] > 0:\n",
    "        return s + A[n]\n",
    "    return s"
   ]
